{
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "useTabs": false,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "always",
    "endOfLine": "lf",
    "printWidth": 100,
    "proseWrap": "preserve",
    "htmlWhitespaceSensitivity": "css",
    "plugins": [
        "prettier-plugin-tailwindcss"
    ],
    "tailwindConfig": "./tailwind.config.ts"
}